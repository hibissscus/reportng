        </packages>
    </test>
    <test parallel="classes" name="partial tests 70%">
        <packages>
            <package name="testee.it.reportng.example.partial.p70"/>
        </packages>
    </test>
    <test parallel="classes" name="partial tests 60%">
        <packages>
            <package name="testee.it.reportng.example.partial.p60"/>
        </packages>
    </test>
    <test parallel="classes" name="partial tests 50%">
        <packages>
            <package name="testee.it.reportng.example.partial.p50"/>
        </packages>
    </test>
    <test parallel="classes" name="partial tests 40%">
